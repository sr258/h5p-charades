{
  "name": "h5p-charades",
  "version": "0.1.0",
  "description": "A H5P game activity for guessing words",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production ./node_modules/.bin/webpack -p",
    "format:check": "prettier --check {src,tests}/**/*.{js,css}",
    "format": "prettier --write {src,tests}/**/*.{js,css}",
    "lint": "eslint {src,tests}/**/*.js",
    "test:coverage": "jest --coverage=true --collectCoverageFrom=src/**/*.js",
    "test": "jest",
    "watch": "./node_modules/.bin/webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sr258/h5p-charades.git"
  },
  "keywords": [
    "h5p",
    "library"
  ],
  "author": "Sebastian Rettig",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "babel-loader": "^8.0.6",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.17.0",
    "file-loader": "^5.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.19.1",
    "resolve-url-loader": "^3.1.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "@babel/preset-react": "^7.7.4",
    "jest": "^24.9.0",
    "jest-babel": "^1.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
}
